<?xml version="1.0" encoding="utf-8"?>
     <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
         android:id="@+id/tab_ticketpieces"
         android:layout_width="fill_parent"
         android:layout_height="fill_parent"
         android:gravity="center_horizontal"
         android:orientation="vertical" >


         <LinearLayout
             android:layout_width="fill_parent"
             android:layout_height="62dp"
             android:orientation="horizontal" >

             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/mainform_num_text" />

             <EditText
                 android:id="@+id/mform_num_show_edt"
                 android:layout_width="203dp"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="10dp"
                 android:singleLine="true"
                 android:textSize="18sp" >
             </EditText>
         </LinearLayout>

         <LinearLayout
             android:layout_width="270dp"
             android:layout_height="62dp"
             android:layout_marginRight="27dp"
             android:orientation="horizontal" >


             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/otform_num_text" />

             <Button
                 android:id="@+id/delfrom_num_btn"
                 android:layout_width="20dp"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="140dp"
                 android:layout_weight="0.07"
                 android:text="@string/delform_num_text" />
         </LinearLayout>




         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="125dp"
             android:orientation="horizontal" >


             <EditText
                 android:id="@+id/mform_num_show_edt"
                 android:layout_width="276dp"
                 android:layout_height="110dp" >

             </EditText>
         </LinearLayout>

                 <LinearLayout
                     android:layout_width="fill_parent"
                     android:layout_height="61dp"
                     android:orientation="horizontal" >

             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/otform_num_text" />

             <EditText
                 android:id="@+id/otform_num_edt"
                 android:layout_width="208dp"
                 android:layout_height="wrap_content"
                 android:layout_marginLeft="10dp"
                 android:singleLine="true"
                 android:textSize="18sp" >
             </EditText>
         </LinearLayout>





            <LinearLayout
                android:layout_width="188dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

             <TextView
                 android:layout_width="50dp"
                 android:layout_height="wrap_content"
                 android:text="@string/total_text" />
              <TextView
                 android:id="@+id/otform_num_edt"
                 android:layout_width="50dp"
                 android:layout_height="wrap_content"
                 android:textColor="#FF0000"
                 android:text="@string/zero_text"/>
               <TextView
                 android:layout_width="50dp"
                 android:layout_height="wrap_content"
                 android:text="@string/piece_text" />

         </LinearLayout>
    <View  
           android:layout_height="1px"
           android:background="#FFFFFF"    
           android:layout_width="fill_parent"></View>
          <RelativeLayout
           android:layout_width="fill_parent"
           android:layout_height="match_parent"
           android:focusable="true" >

           <Button
               android:id="@+id/back_btn"
               android:layout_width="100dp"
               android:layout_height="60dp"
               android:layout_alignParentBottom="true"
               android:text="@string/back_text" />

           <Button
               android:id="@+id/save_btn"
               android:layout_width="100dp"
               android:layout_height="60dp"
               android:layout_alignParentBottom="true"
               android:layout_alignParentRight="true"
               android:text="@string/save_text" />
       </RelativeLayout>
        
     </LinearLayout>
  